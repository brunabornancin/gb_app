            a.dropdown-item(href="") Instructors
        li.nav-item.dropdown
          a.nav-link.dropdown-toggle(id='programs', data-toggle='dropdown', aria-haspopup='true', aria-expanded='false') Programs
          .dropdown-menu(aria-labelledby='programs')
            a.dropdown-item(href="") Adults
            a.dropdown-item(href="") Kids & Teens
            a.dropdown-item(href="") Private Training
        li.nav-item: a.nav-link(href="#") Events & News
        li.nav-item: a.nav-link(href="#") Gallery
        li.nav-item: a.nav-link(href="#") Contact
        
      a.btn.btn-primary.btn-lg(href="/register") GET A FREE INTRO CLASS        